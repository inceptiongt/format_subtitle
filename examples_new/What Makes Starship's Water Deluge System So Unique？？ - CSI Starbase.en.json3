    "dDurationMs": 1333,
    "segs": [ {
      "utf8": " walls is actually fairly shallow."
    } ]
  }, {
    "tStartMs": 981433,
    "dDurationMs": 2000,
    "segs": [ {
      "utf8": " They are typically about three\n to five feet deep."
    } ]
  }, {
    "tStartMs": 984333,
    "dDurationMs": 2900,
    "segs": [ {
      "utf8": " Once the formwork is in place, concrete\n is poured on either side."
    } ]
  }, {
    "tStartMs": 987799,
    "dDurationMs": 2767,
    "segs": [ {
      "utf8": " After it's hardened to an acceptable\n level, the formwork is removed"
    } ]
